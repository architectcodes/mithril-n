{ "name": "mithril-n"
, "version": "1.0.0"
, "description": "Pure DOM nodes in Mithril templates."
, "license": "MIT"

, "dependencies":
  { "mithril": ">=0.1.0 <0.3.0"
  }

, "scripts":
  { "prepublish": "gulp build"
  , "test": "jshint *.js source/*.js test/*.js && browserify test/index.js --transform babelify | tape-run --browser=phantom | tap-spec"
  , "test-debug": "echo 'Now navigate your browser to http://localhost:8001/ to run the tests â€“ and see their results in the console.' && browserify test/index.js --debug --transform babelify | tape-run --port=8001"
  }

, "devDependencies":
  { "browserify": "10.0.0"
  , "babelify": "6.0.2"
  , "tape-run": "1.0.0"
  , "tape-catch": "1.0.4"
  , "babel": "5.2.9"
  , "tap-spec": "2.2.0"
  , "jshint": "2.7.0"

  , "gulp": "3.8.11"
  , "gulp-babel": "5.1.0"
  , "gulp-help": "1.3.4"
  , "gulp-uglify": "1.2.0"
  , "gulp-rename": "1.2.2"
  }

, "main": "dist.es5/n.js"
, "files":
  [ "dist"
  , "dist.es5"
  , "Readme.md"
  , "License.md"
  ]

, "contributors":
  [ { "name": "Tomek Wiszniewski"
    , "email": "t.wiszniewski@gmail.com"
    }
  , { "name": "Barney Carroll"
    , "email": "barney.carroll@gmail.com"
    }
  ]

, "keywords":
  [ "mithril"
  , "dom"
  , "virtual-dom"
  , "xml"
  , "svg"
  , "rendering"
  , "performance"
  , "view"
  , "template"
  ]
, "repository":
  { "type": "git"
  , "url": "https://github.com/tomekwi/mithril-n.git"
  }
}
